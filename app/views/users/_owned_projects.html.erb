<div class= "container">
	<dl>
		<h2>Owned Projects</h2>
		<% current_user.projects.each do |project| %>
			<div class="well well-sm">
				<dt class="h3"> Project title:
					<%= link_to project.title, project_path(project.id)%>
				</dt>
					<dd>Description: <%= project.description %>></dd>
			</div>
		<% end %>
	</dl>
</div>